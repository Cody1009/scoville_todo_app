umn":15}}],"sources":{"components/Forms/ListForm/ListForm.js":"import React from 'react';\nimport './ListForm.css'\n\nconst listForm = (props) => {\n\n\n    const todos_list = props.todos.map(todo => {\n        const doneTrueStyle ={\n            color: 'blue'\n        };\n\n        const doneFalseStyle = {\n            color:'red'\n        };\n\n        return (\n\n                <p key={todo.id}>\n                    {todo.id}:\n                    <input type=\"text\"\n                           value={todo.content}\n                           onChange={(event) => props.updateTodo(event, todo.id)}\n                        //onChange={props.contentChanged}\n                    />\n\n\n\n                    <button onClick={() => {\n                        props.deleteTodo(todo.id)\n                    }}>\n                        DELETE!\n                    </button>\n\n                    <button\n                        onClick={() => {\n                            props.toggleStatus(todo.id)\n                        }}\n                    >\n                        Change Status!\n                    </button>\n\n                    <span style={ todo.done? doneTrueStyle: doneFalseStyle}> done status</span>\n\n                </p>\n\n        );\n    });\n\n    return (\n        <div className=\"list\">\n            <h1>These tasks below are not done yet</h1>\n\n            {todos_list}\n        </div>\n    )\n\n};\n\nexport default listForm;"},"lineCount":null}},"hash":"bac3e127a9fba619cd59cad4097d19e8","cacheData":{"env":{}}}